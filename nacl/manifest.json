{
  "name": "WebP2P",
  "description": "When your P wants a P with your Web",
  "minimum_chrome_version": "19",
  "manifest_version": 2,
  "version": "0.0.0.1",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "128": "img/icon_128.png"
  },
  "browser_action": {},
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "<all_urls>",
    "unlimitedStorage"
  ]
  /**
  "app": {
    "urls": [
      "http://bumping.cs.washington.edu:5103/"
    ],
    "launch": {
      "web_url": "http://bumping.cs.washington.edu:5103/nacl_test.html"
    }
  }
  **/
}
